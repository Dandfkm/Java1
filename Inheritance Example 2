public class MyClass {

    public static void main(String args[]) {
      Company a = new Company();
      Company b = new eBay();

      a.address();
      b.address();
  }
}

class Company() {

  public void address() {
    System.out.println("This is Address of Crunchify Company...");
  }
}

class eBay extends Company() {

  public void address() {

    System.out.println("This is eBay's Address...");
  }
}
